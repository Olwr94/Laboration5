<Window x:Class="Laboration5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laboration5"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,0.4,0" Height="320" VerticalAlignment="Top">
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FFA3913D" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <ListBox HorizontalAlignment="Left" Height="158" Margin="25,48,0,0" VerticalAlignment="Top" Width="115"/>
        <ListBox HorizontalAlignment="Left" Height="114" Margin="388,48,0,0" VerticalAlignment="Top" Width="115"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="175,48,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="182" FontStyle="Italic" TextDecorations="{x:Null}"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="175,87,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="182" FontStyle="Italic"/>
        <Label Content="Laboration 5" HorizontalAlignment="Left" Margin="183,6,0,0" Width="140" VerticalAlignment="Top" FontWeight="Bold" FontStyle="Italic" HorizontalContentAlignment="Center" FontFamily="Rockwell" FontSize="20"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="25,10,0,0" TextWrapping="Wrap" Text="Users" VerticalAlignment="Top" Width="115" RenderTransformOrigin="0.268,0.464" TextDecorations="{x:Null}" HorizontalContentAlignment="Center" FontFamily="Elephant"/>
        <TextBox HorizontalAlignment="Left" Height="17" Margin="25,31,0,0" TextWrapping="Wrap" Text="0/10" VerticalAlignment="Top" Width="115" FontSize="11" HorizontalContentAlignment="Center"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="388,7,0,0" TextWrapping="Wrap" Text="Admin" VerticalAlignment="Top" Width="115" RenderTransformOrigin="0.268,0.464" TextDecorations="{x:Null}" HorizontalContentAlignment="Center" FontFamily="Elephant"/>
        <TextBox HorizontalAlignment="Left" Height="17" Margin="388,31,0,0" TextWrapping="Wrap" Text="0/4" VerticalAlignment="Top" Width="115" FontSize="11" HorizontalContentAlignment="Center"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="45,224,0,0" VerticalAlignment="Top" Width="130" Height="46" FontFamily="Elephant"/>
        <Button Content="Change" HorizontalAlignment="Left" Margin="195,224,0,50" VerticalAlignment="Center" Width="130" Height="46" FontFamily="Elephant" HorizontalContentAlignment="Center"/>
        <Button Content="Remove" HorizontalAlignment="Left" Margin="345,224,0,0" VerticalAlignment="Top" Width="130" Height="46" FontFamily="Elephant"/>

    </Grid>
</Window>
